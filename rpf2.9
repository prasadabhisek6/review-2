    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t!=0)
    {
      int n = sc.nextInt();
      int arr[] = {1,2,5,10,20,50,100,500,1000};
      int note_count=0;
      for(int i=8;i>=0;i--)
