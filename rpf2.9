#include <stdio.h>

int main()
{
  int t;
  scanf("%d",&t);
  while(t--)
  {
    int n;
    scanf("%d",&n);
    int arr[9]={1,2,5,10,20,50,100,500,1000};
    int note_count=0;
    for(int i=8;i>=0;i--)
    {
      if(arr[i]<=n)
      {
        note_count += n/arr[i];
        n %= arr[i];
      }
    }
    printf("%d\n",note_count);
  }
  return 0;
}

